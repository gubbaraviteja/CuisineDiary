  {
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "max-len": ["error", 120],
    "no-console": "off",
    "prefer-arrow-callback": "off",
    "linebreak-style": "off",
    "indent": ["error", 4]
  },
  "globals": {
    "_": true
  }
}